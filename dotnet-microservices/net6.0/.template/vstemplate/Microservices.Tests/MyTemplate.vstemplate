<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>Microservice.Tests</Name>
    <Description>GoodToCode Microservice template contains a simple microservice solution and pre-setup projects. Built on DDD, CQRS with SOLID in mind.</Description>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>
    </ProjectSubType>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>Microservice.Tests</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <CreateInPlace>true</CreateInPlace>
    <Icon>__TemplateIcon.ico</Icon>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="Microservice.Tests.csproj" File="Microservice.Tests.csproj" ReplaceParameters="true">
      <Folder Name="Application" TargetFolderName="Application">
        <Folder Name="Associate" TargetFolderName="Associate">
          <ProjectItem ReplaceParameters="true" TargetFileName="BusinessQuerySteps.cs">BusinessQuerySteps.cs</ProjectItem>
          <ProjectItem ReplaceParameters="true" TargetFileName="BusinessSaveCommandSteps.cs">BusinessSaveCommandSteps.cs</ProjectItem>
        </Folder>
      </Folder>
      <Folder Name="Domain" TargetFolderName="Domain">
        <Folder Name="Associate" TargetFolderName="Associate">
          <ProjectItem ReplaceParameters="true" TargetFileName="AssociateAggregateSteps.cs">AssociateAggregateSteps.cs</ProjectItem>
        </Folder>
      </Folder>
      <Folder Name="Factories" TargetFolderName="Factories">
        <ProjectItem ReplaceParameters="true" TargetFileName="AppConfigurationFactory.cs">AppConfigurationFactory.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="DbContextFactory.cs">DbContextFactory.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="FileFactoryService.cs">FileFactoryService.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="HttpRequestFactory.cs">HttpRequestFactory.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="PathFactory.cs">PathFactory.cs</ProjectItem>
      </Folder>
      <Folder Name="Infrastructure" TargetFolderName="Infrastructure">
        <Folder Name="Associate" TargetFolderName="Associate">
          <ProjectItem ReplaceParameters="true" TargetFileName="BusinessCreateSteps.cs">BusinessCreateSteps.cs</ProjectItem>
          <ProjectItem ReplaceParameters="true" TargetFileName="BusinessDeleteSteps.cs">BusinessDeleteSteps.cs</ProjectItem>
          <ProjectItem ReplaceParameters="true" TargetFileName="BusinessesGetSteps.cs">BusinessesGetSteps.cs</ProjectItem>
          <ProjectItem ReplaceParameters="true" TargetFileName="BusinessGetByKeySteps.cs">BusinessGetByKeySteps.cs</ProjectItem>
          <ProjectItem ReplaceParameters="true" TargetFileName="BusinessUpdateSteps.cs">BusinessUpdateSteps.cs</ProjectItem>
        </Folder>
      </Folder>
      <Folder Name="Steps" TargetFolderName="Steps">
        <ProjectItem ReplaceParameters="true" TargetFileName="IAggregateSteps.cs">IAggregateSteps.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="ICommandSteps.cs">ICommandSteps.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="ICrudSteps.cs">ICrudSteps.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="IQuerySteps.cs">IQuerySteps.cs</ProjectItem>
      </Folder>
      <ProjectItem ReplaceParameters="true" TargetFileName="appsettings.Development.json">appsettings.Development.json</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="appsettings.json">appsettings.json</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="appsettings.Production.json">appsettings.Production.json</ProjectItem>
    </Project>
  </TemplateContent>
</VSTemplate>